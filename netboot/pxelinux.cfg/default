default http://192.168.1.200:80/isolinux/vesamenu.c32
#prompt 1
timeout 300

display http://192.168.1.200:80/isolinux/boot.msg

menu background http://192.168.1.200:80/isolinux/splash.jpg
menu title Welcome to PXEServer http://192.168.1.200:80
menu vshift 2
menu rows 20
menu margin 4
#menu hidden
menu helpmsgrow 25
menu tabmsgrow 22
menu color border 0 #ffffffff #00000000
menu color sel 0 #ffffffff #00000000 none
menu color title 0 #ffffffff #00000000 none
menu color tabmsg 0 #ffffffff #00000000 none
menu color unsel 0 #84b8ffff #00000000 none
menu color hotsel 0 #84b8ffff #00000000 none
menu color hotkey 0 #ffffffff #00000000 none


label CentOS6
  menu label Install CentOS 6 With Kickstart
  kernel http://192.168.1.200:80/centos/6/isolinux/vmlinuz
  append initrd=http://192.168.1.200:80/centos/6/isolinux/initrd.img ks=http://192.168.1.200:80/linux/ks/centos6.ks
label CentOS7
  menu label Install CentOS 7 With Kickstart
  menu default
  kernel http://192.168.1.200:80/centos/7/isolinux/vmlinuz
  append initrd=http://192.168.1.200:80/centos/7/isolinux/initrd.img ramdisk_size=300000 ks=http://192.168.1.200:80/linux/ks/centos7.ks text
label CentOS8
  menu label Install CentOS 8 With Kickstart
  kernel http://192.168.1.200:80/centos/8/isolinux/vmlinuz
  append initrd=http://192.168.1.200:80/centos/8/isolinux/initrd.img inst.repo=http://192.168.1.200:80/centos/8 inst.ks=http://192.168.1.200:80/linux/ks/centos8.ks text
menu separator # insert an empty line
